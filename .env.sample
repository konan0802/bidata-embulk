# Database connections
DB_HOST=your-postgres-host.com
DB_PORT=5432
DB_USER=your_username
DB_PASSWORD=your_password
DB_NAME=your_database

MYSQL_HOST=your-mysql-host.com
MYSQL_PORT=3306
MYSQL_USER=your_mysql_username
MYSQL_PASSWORD=your_mysql_password
MYSQL_DB=your_mysql_database

MONGODB_HOST=your-mongodb-host.com
MONGODB_PORT=27017
MONGODB_USER=your_mongodb_username
MONGODB_PASSWORD=your_mongodb_password
MONGODB_DB=your_mongodb_database

# Redshift data warehouse
REDSHIFT_HOST=your-redshift-cluster.region.redshift.amazonaws.com
REDSHIFT_USER=your_redshift_username
REDSHIFT_PASSWORD=your_redshift_password
REDSHIFT_DB=your_redshift_database
REDSHIFT_SCHEMA=public

# AWS S3 for temporary storage
S3_BUCKET=your-s3-bucket-name

# IAM role for Redshift COPY command
REDSHIFT_COPY_IAM_ROLE_NAME=your-redshift-iam-role
# AWS credentials (if not using IAM roles)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_DEFAULT_REGION=ap-northeast-1
